{
  "name": "cueb",
  "productName": "CueB Controller Software",
  "version": "4.2.0",
  "description": "Theatre CueLight system built on an Arduino",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon/scaled/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . cueb --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icon/scaled/icon.ico --prune=true --out=release-builds --version-string.CompanyName=JBithell --version-string.FileDescription=CueB Show Control --version-string.ProductName=\"CueB Controller Software\"",
    "package-linux": "electron-packager . cueb --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon/scaled/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Jbithell/CueB"
  },
  "author": "JBithell",
  "devDependencies": {
    "electron": "^3.1.1",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.0.1",
    "electron-rebuild": "^1.8.4",
    "windows-build-tools": "^5.1.0"
  },
  "dependencies": {
    "electron-settings": "3.2.0",
    "fitty": "^2.2.6",
    "jquery": "^3.3.1",
    "osc": "^2.2.0",
    "serialport": "^7.1.3",
    "socket.io": "^3.0.5"
  }
}
